include core/_mixins
block variables

doctype html
html(lang='ru')
    head
        meta(charset="utf-8")
        title= pageTitle
        meta(content="" name="author")
        meta(content="" name="description")
        meta(content="" name="keywords")
        meta(content="width=device-width, initial-scale=1" name="viewport")
        meta(content="ie=edge" http-equiv="x-ua-compatible")
        meta(name="format-detection" content="telephone=no")

        //- favicon block start

        //- favicon block end
        
        link(rel='stylesheet' href='./css/foundation.css')
        link(rel='stylesheet' href='./css/app.css')
        

    body
        div(class='wrapper', id=page)
            block content
        script(src='./js/foundation.js' defer)
        script(src='./js/app.js' defer)
        script.
          function initMap() {
            position = {lat: 40.7143528, lng: -74.0059731};
              // Create a map object and specify the DOM element for display.
              var map = new google.maps.Map(document.getElementById('map'), {
                  center: position,
                  scrollwheel: false,
                  zoom: 10,
                  disableDefaultUI: true
              });
              var image  = {
                url : 'data:image/png;base64,',
                size: new google.maps.Size(27, 27),
                anchor: new google.maps.Point(0, 32),
                origin: new google.maps.Point(0, 0),

              }
               var marker = new google.maps.Marker({
                position: position,
                map: map,
                title: 'Sedona',
                icon : image
              });
          }
        script(src='https://maps.googleapis.com/maps/api/js?key=AIzaSyAUWCNbZ4BqkZYhXdQuMcWjp8QTTTJiiuQ&callback=initMap', async='', defer='')


         
